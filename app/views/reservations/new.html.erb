<% provide(:title, "Upload room reservation") %>
<h1>Upload room reservation</h1>

<div class="row">
  <div class="span6 offset3">



	<%= form_for(:uploaded_file, url: reservations_path, :html => { :multipart => true }) do |f| %> 

	    <%= f.file_field :uploaded_file  %>
	  
	  <p><%= f.submit 'Fetch data', class: "btn btn-large btn-primary" %></p>

	<% end %>



  </div>
</div>